
.sidebar-group.is-sub-group.depth-1{
  & > .sidebar-heading:not(.clickable){
    opacity: 1;
  }
  .sidebar-heading{
    span{
      font-size: 1.1em;
      font-weight: bold;
    }
  }
}
.theme-container{
  -moz-user-select:none; /*火狐*/
  -webkit-user-select:none; /*webkit浏览器*/
  -ms-user-select:none; /*IE10*/
  -khtml-user-select:none; /*早期浏览器*/
  user-select:none;
  -webkit-touch-callout: none;
}