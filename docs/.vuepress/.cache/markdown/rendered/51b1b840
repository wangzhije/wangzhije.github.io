g":"主要流程","link":"#主要流程","children":[{"level":3,"title":"Yaml","slug":"yaml","link":"#yaml","children":[]}]},{"level":2,"title":"Actions Secrets","slug":"actions-secrets","link":"#actions-secrets","children":[{"level":3,"title":"创建 Actions Secrets","slug":"创建-actions-secrets","link":"#创建-actions-secrets","children":[]}]}],"links":[{"raw":"!./../tokens.md","absolute":"/blog/notes/tools/2.Github/tokens.md","relative":"notes/tools/2.Github/tokens.md"}]}}
